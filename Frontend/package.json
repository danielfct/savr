{
  "name": "savr",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "homepage": "https://danielfct.github.io/savr",
  "dependencies": {
    "@date-io/moment": "0.0.2",
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "assets": "^3.0.0",
    "classnames": "2.2.6",
    "components": "^0.1.0",
    "hal": "^1.2.0",
    "halfred": "^1.1.1",
    "history": "4.7.2",
    "material-ui": "^0.20.2",
    "material-ui-pickers": "^2.0.2",
    "moment": "^2.22.2",
    "node-sass-chokidar": "1.3.3",
    "nouislider": "12.0.0",
    "npm-run-all": "4.1.3",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-custom-scrollbars": "latest",
    "react-datetime": "2.15.0",
    "react-dom": "16.5.2",
    "react-redux-loading-bar": "^4.1.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.5",
    "react-slick": "0.23.1",
    "react-swipeable-views": "0.13.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "variables": "^1.0.1",
    "views": "^1.0.2",
    "react-redux": "latest"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "gh-pages": "^2.0.1",
    "redux-logger": "^3.0.6"
  },
  "optionalDependencies": {
    "ajv": "5.0.0",
    "babel-eslint": "7.2.3",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.14.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass-chokidar src/assets/scss/savr.scss dist/savr.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
